percentage_of_peaks_overlapped_with_DHSs = \VAR{ peaks.dhspercentage }
%- endif

%- if section_name == "velcro"
[Velcro]
verlcro overlap peak number is: \VAR{ peaks.velcro }
verlcro overlap percentage is: \VAR{ peaks.velcropercentage }
%- endif

